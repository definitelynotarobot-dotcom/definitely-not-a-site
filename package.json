{
  "name": "dot-org-hugo-theme",
  "version": "1.0.0",
  "description": "",
  "main": "none.js",
  "scripts": {
    "build": "npx hugo-bin --theme=dot-org-hugo-theme",
    "build:production": "npx hugo-bin --environment=production --themesDir=../.. --baseURL ${npm_config_url}",
    "start": "npx hugo-bin serve --logLevel info --configDir=config --buildDrafts --buildFuture --ignoreCache --disableFastRender --gc --printI18nWarnings --printMemoryUsage --printPathWarnings --printUnusedTemplates --templateMetrics --templateMetricsHints",
    "dev": "npm run dev:start",
    "dev:start": "npx hugo-bin serve --logLevel info --configDir=config --themesDir=../.. --buildDrafts --buildFuture --ignoreCache --disableFastRender --gc --printI18nWarnings --printMemoryUsage --printPathWarnings --printUnusedTemplates --templateMetrics --templateMetricsHints",
    "dev:start:with-pagefind": "npx hugo-bin --baseURL=/ --themesDir=../.. --theme=dot-org-hugo-theme && npm_config_yes=true npx pagefind --site 'public' --output-subdir '../static/pagefind' && npm run dev:start",
    "dev:build": "npx hugo-bin --themesDir=../.. --theme=dot-org-hugo-theme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cncf/dot-org-hugo-theme.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cncf/dot-org-hugo-theme/issues"
  },
  "homepage": "https://github.com/cncf/dot-org-hugo-theme#readme",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "netlify-cli": "^22.3.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  },
  "devDependencies": {
    "hugo-bin": "^0.149.2",
    "prettier": "^3.6.2",
    "sass": "^1.94.0"
  },
  "type": "commonjs"
}
